{
  "name": "@reustleco/dojo-frontend-component-admin-header",
  "version": "1.0.2",
  "description": "The standard header for Open edX",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/@reustleco"
  },
  "scripts": {
    "build": "make build",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage",
    "prepare": "husky install"
  },
  "files": [
    "/dist"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reustleco/dojo-frontend-component-admin-header.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/reustleco/dojo-frontend-component-admin-header/issues"
  },
  "homepage": "https://github.com/reustleco/dojo-frontend-component-admin-header/#readme",
  "devDependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.1.0",
    "@edx/frontend-build": "5.6.14",
    "@edx/frontend-platform": "1.14.1",
    "@edx/paragon": "12.8.0",
    "@testing-library/dom": "7.31.2",
    "@testing-library/jest-dom": "5.15.1",
    "@testing-library/react": "10.3.0",
    "codecov": "3.8.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-chain": "1.1.5",
    "lint-staged": "^12.4.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.6",
    "react-test-renderer": "16.14.0",
    "reactifex": "1.1.1",
    "redux": "4.1.2",
    "redux-saga": "1.1.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.3.1",
    "react-responsive": "8.2.0",
    "react-router-dom": "5.3.0",
    "react-transition-group": "4.4.2"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^1.8.0",
    "@edx/paragon": ">= ^7.0.0 < 13.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
